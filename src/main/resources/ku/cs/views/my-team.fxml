<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.MyteamController">
   <children>
      <Button fx:id="teamButton" layoutX="990.0" layoutY="23.0" mnemonicParsing="false" style="-fx-background-color: #ffffff;" text="My Team">
         <font>
            <Font name="System Bold Italic" size="30.0" />
         </font>
      </Button>
      <ImageView fx:id="groupImage" fitHeight="58.0" fitWidth="62.0" layoutX="1165.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/Group.png" />
         </image>
      </ImageView>
      <ImageView fx:id="augustyouthImage" fitHeight="479.0" fitWidth="221.0" layoutX="94.0" layoutY="167.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/augustyouth.png" />
         </image>
      </ImageView>
      <ImageView fx:id="frameImage" fitHeight="479.0" fitWidth="221.0" layoutX="379.0" layoutY="167.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/frame.png" />
         </image>
      </ImageView>
      <ImageView fx:id="peteImage" fitHeight="479.0" fitWidth="221.0" layoutX="680.0" layoutY="167.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/pete.png" />
         </image>
      </ImageView>
      <ImageView fx:id="nickImage" fitHeight="479.0" fitWidth="221.0" layoutX="989.0" layoutY="167.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/nick.png" />
         </image>
      </ImageView>
      <Label fx:id="teamnameLabel" layoutX="542.0" layoutY="86.0" text="Monday I miss you" textFill="#16559e">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <ImageView fx:id="backImage" fitHeight="42.0" fitWidth="48.0" layoutX="16.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/Back.png" />
         </image>
      </ImageView>
      <Button fx:id="backbutton" layoutX="12.0" layoutY="18.0" mnemonicParsing="false" onAction="#onBackButtonClick" opacity="0.0" prefHeight="68.0" prefWidth="57.0" style="-fx-background-color: #FFFFFF;">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
