<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.advisor.AdvisorAppealController">
   <children>
      <Label layoutX="564.0" layoutY="53.0" text="คำร้องล่าสุด">
         <font>
            <Font name="Arial Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutX="257.0" layoutY="107.0" prefHeight="485.0" prefWidth="776.0" style="-fx-background-radius: 10;">
         <columns>
            <TableColumn prefWidth="119.0" text="studentID" />
            <TableColumn prefWidth="185.0" text="studentSignature" />
         </columns>
      </TableView>
      <Button fx:id="acceptButtonClick" layoutX="553.0" layoutY="618.0" mnemonicParsing="false" style="-fx-background-color: #317127; -fx-background-radius: 20;" text="อนุมัติ" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="rejectButtonClick" layoutX="637.0" layoutY="618.0" mnemonicParsing="false" style="-fx-background-color: FA0202; -fx-background-radius: 20;" text="ไม่อนุมัติ" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="20.0" />
         </font>
      </Button>
      <ImageView fitHeight="28.0" fitWidth="31.0" layoutX="25.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../images/Leftarrow.png" />
         </image>
      </ImageView>
      <Button fx:id="Backbutton" layoutX="12.0" layoutY="24.0" mnemonicParsing="false" onAction="#onBackButtonClick" opacity="0.0" text="Button" />
   </children>
</AnchorPane>
